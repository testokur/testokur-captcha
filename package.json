{
  "name": "testokur-captcha",
  "version": "1.0.0",
  "description": "testokur captcha api",
  "main": "index.js",
  "repository": "https://github.com/testokur/captcha",
  "author": "Nazmi Altun",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node dist",
    "lint": "eslint src --fix",
    "build": "rimraf dist && babel src --copy-files --out-dir dist --source-maps",
    "start:dev": "nodemon --watch src --exec babel-node src --source-maps"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,yml,yml,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "global-cache": "^1.2.1",
    "gm": "^1.23.1",
    "helmet": "^4.6.0",
    "http-codes": "^1.0.0",
    "svg-captcha": "^1.4.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/node": "^7.14.5",
    "@babel/preset-env": "^7.14.4",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1"
  }
}
